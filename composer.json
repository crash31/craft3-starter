{
	"require": {
		"craftcms/cms": "^3.4.1",
		"craftcms/redactor": "2.8.6",
		"craftsnippets/craft-image-toolbox": "1.1.1",
		"misterbk/mix": "1.5.2",
		"mmikkel/cp-field-inspect": "1.2.4",
		"nystudio107/craft-seomatic": "3.3.40",
		"spicyweb/craft-neo": "2.9.6",
		"vaersaagod/dospaces": "1.1.3",
		"vlucas/phpdotenv": "^3.4.0"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "7.4"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	}
}
