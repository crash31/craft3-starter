{
	"require": {
		"aelvan/imager": "v2.3.1",
		"aelvan/imager-do-spaces-driver": "1.0.0",
		"craftcms/cms": "^3.4.5",
		"craftcms/redactor": "2.5.0",
		"misterbk/mix": "1.5.2",
		"mmikkel/cp-field-inspect": "1.0.7",
		"nystudio107/craft-seomatic": "3.2.41",
		"spicyweb/craft-neo": "2.6.0",
		"vaersaagod/dospaces": "1.1.2",
		"vlucas/phpdotenv": "^3.4.0"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "7.0"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	}
}
